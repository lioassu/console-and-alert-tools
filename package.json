{
  "name": "console-alert-tool",
  "displayName": "console and alert tools",
  "description": "",
  "version": "0.0.7",
  "publisher": "lioy",
  "icon":"icon.webp",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.log",
        "title": "console-alert-tool.log"
      },
      {
        "command": "extension.alert",
        "title": "console-alert-tool.alert"
       
      }
    ],
    "keybindings": [
      {
        "command": "extension.log",
        "key": "alt+l",
        "mac": "alt+l"
      },
      {
        "command": "extension.alert",
        "key": "alt+k",
        "mac": "alt+k"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Console Alert Tool",
      "properties": {
        "console-alert-tool.prefix": {
          "type": "string",
          "default": "",
          "description": "自定义前缀"
        },
        "console-alert-tool.suffix": {
          "type": "string",
          "default": "",
          "description": "自定义后缀"
        }
      }}
    
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "publish": "vsce publish",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}